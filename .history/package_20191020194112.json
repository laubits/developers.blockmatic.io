  "scripts": {
    "server-dev": "nodemon server/index.js",
    "build-prod": "yarn build-css && react-scripts build && mv prod/ last_prod/ && mv build/ prod/ && rm -rf last_prod",
    "server-prod": "NODE_ENV=production pm2 start server/index.js --name liveProduction -o ./output.log -e ./error.log",
    "strapi-prod": "pm2 start strapi --no-automation --name strapi -- start",
    "dev": "concurrently 'yarn watch-css' 'yarn start'",
    "build-css": "node-sass-chokidar --include-path ./node_modules src/ -o src/ --match-regex '/^(?!_).+/'",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start": "NODE_ENV=development react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "flow": "flow"
  },
